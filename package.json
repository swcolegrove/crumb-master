{
  "name": "crumb-master",
  "version": "1.0.0",
  "description": "agile planning poker",
  "author": "",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:watch": "nodemon --watch src --exec \"env-cmd .env vue-cli-service build\" --ext scss,js,vue",
    "test": "echo \"TESTS??? We don't need no stinkin' tests!\" && exit 1",
    "lint": "vue-cli-service lint",
    "dev": "concurrently \"npm run build:watch\" \"env-cmd .env npm start\"",
    "heroku:init": "heroku git:remote -a crumb-master",
    "publish": "git push heroku master",
    "start": "node index.js",
    "heroku-postbuild": "npm run build"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.3",
    "bootstrap": "^4.3.1",
    "express": "^4.16.4",
    "redis": "^2.8.0",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "env-cmd": "^8.0.2",
    "eslint": "^5.14.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.10",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "serve-static": "^1.13.2",
    "vue": "^2.6.6",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/swcolegrove/crumb-master/issues"
  },
  "homepage": "https://github.com/swcolegrove/crumb-master#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swcolegrove/crumb-master.git"
  }
}
